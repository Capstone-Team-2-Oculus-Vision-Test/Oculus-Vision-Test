<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="False">
    <Style src="project://database/Assets/PractitionerMenu/UI/StyleSheets/NewTest.uss?fileID=7433441132597879392&amp;guid=abe541b846cf41a4fa463266da1fe5b7&amp;type=3#NewTest" />
    <Style src="project://database/Assets/PractitionerMenu/UI/StyleSheets/BackButton.uss?fileID=7433441132597879392&amp;guid=cf94f838b797fee49a7c80202534a9b8&amp;type=3#BackButton" />
    <ui:VisualElement name="Container" style="width: 100%; height: 100%; flex-direction: column; background-color: rgb(48, 48, 48); align-items: stretch; flex-wrap: nowrap; flex-grow: 1; justify-content: space-between;">
        <ui:VisualElement name="VariableContainer" style="flex-direction: row; justify-content: flex-start; flex-wrap: wrap; align-items: flex-start; flex-grow: 1;">
