                <ui:ListView focusable="true" name="patientList" fixed-item-height="40" show-add-remove-footer="false" show-border="true" style="width: 300px; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0); border-top-width: 2px;" />
            </ui:VisualElement>
            <ui:VisualElement style="flex-direction: row; justify-content: space-around; flex-grow: 1;">
                <ui:VisualElement name="PatientInfo" style="flex-wrap: wrap; flex-direction: column;">
                    <ui:TextField picking-mode="Ignore" label="First Name" name="FirstName" style="align-items: stretch;" />
                    <ui:TextField picking-mode="Ignore" label="Last Name" name="LastName" style="align-items: stretch;" />
                    <ui:TextField picking-mode="Ignore" label="Age" name="Age" />
                    <ui:TextField picking-mode="Ignore" label="Sex" name="Sex" />
                    <ui:TextField picking-mode="Ignore" label="Patient ID" name="ID" style="align-items: stretch;" />
                </ui:VisualElement>
                <ui:VisualElement name="TestInfo">
                    <ui:DropdownField label="Eye" index="0" choices="Left, Right" name="SelectEye" class="unity-base-field" />
                    <ui:DropdownField label="Test" index="0" name="SelectTestType" choices="20-2, 30-2" class="unity-base-field" />
